{
	"name": "Rich Browser Media Presence",
	"description": "Displays browser media as rich presence on your desktop Discord client",
	"version": "1.0.0",
	"author": "prototype-A",
	"manifest_version": 2,
	"background": {
		"page": "/popup/background.html"
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": "/icons/icon_red.png",
		"default_popup": "/popup/popup.html"
	},
	"icons": {
		"48": "/icons/icon_red.png",
		"96": "/icons/icon_red.png"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"webNavigation",
		"https://www.youtube.com/watch?v=*",
		"https://www.nicovideo.jp/watch/sm*"
	]
}